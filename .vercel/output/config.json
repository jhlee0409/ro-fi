{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/generate-story/?$",
			"dest": "_render"
		},
		{
			"src": "^/novels/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/novels/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	],
	"images": {
		"sizes": [
			400,
			800,
			1200
		],
		"domains": [],
		"remotePatterns": []
	}
}